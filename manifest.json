{
  "manifest_version": 3,
  "name": "Simple Extension",
  "version": "1.0",
  "description": "Change webpage background color",
  "permissions": [
    "activeTab"
  ],"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}